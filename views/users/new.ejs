<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
  </head>
  <body>
    <h1>Create User</h1>
    <hr>
    <form  action="/users" method="POST">
      <div>
      <label>
        Email
        <input name="email">
      </label>
      </div>
      <div>
      <label>
        First Name
        <input name="firstName">
      </label>
      </div>
      <div>
      <label>
        Last Name
        <input name="lastName">
      </label>
      </div>
      <div>
      <label>
        Date of Birth
        <input name="dob" type="date">
      </label>
      </div>
      <input type="submit" value="Create User">
    </form>
  </body>
</html>
